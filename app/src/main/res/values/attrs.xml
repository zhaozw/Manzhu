<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Topmenu">

        <attr name="toptext" format="string"/>

    </declare-styleable>


    <declare-styleable name="Topiconmenu">

        <attr name="topicontext" format="string"/>
        <attr name="topicon" format="reference"/>
        <attr name="top1iv" format="reference"/>
        <attr name="top2iv" format="reference"/>
        <attr name="top3iv" format="reference"/>
        <attr name="top1tv" format="string"/>
        <attr name="top2tv" format="string"/>
        <attr name="top3tv" format="string"/>

    </declare-styleable>


    <declare-styleable name="SearchTopLin">
        <attr name="searchcontext" format="string"/>
        <attr name="searchtype" format="string"/>
        <attr name="searchtop1iv" format="reference"/>
        <attr name="searchtop2iv" format="reference"/>
        <attr name="searchtop1tv" format="string"/>
        <attr name="searchtop2tv" format="string"/>
        <attr name="isshowmore" format="boolean"/>


    </declare-styleable>

    <declare-styleable name="CircleHead">
        <attr name="circleBorderHeadWidth" format="dimension" />
        <attr name="ringHeadColor" format="color" />
        <attr name="backgroundHeadColor" format="color" />
    </declare-styleable>





    <!-- 末行对齐方式 -->
    <declare-styleable name="AlignTextView">
        <attr name="align" format="enum">
            <enum name="left" value="0"/>
            <enum name="center" value="1"/>
            <enum name="right" value="2"/>
        </attr>
    </declare-styleable>


</resources>