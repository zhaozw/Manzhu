<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/bootmbc"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context="com.zpfan.manzhu.IdleActivity">





    <android.support.v7.widget.RecyclerView
        android:layout_marginTop="45dp"
        android:id="@+id/rv_idel"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>


    <com.zpfan.manzhu.myui.IconTopLin
        android:id="@+id/icontoplin"
        app:topicontext="COS新品"
        app:top1iv ="@mipmap/com_icon_all"
        app:top1tv ="全部"
        app:top2tv ="含定金"
        app:top3tv ="包邮"
        app:top2iv ="@mipmap/com_icon_dep_ept"
        app:top3iv ="@mipmap/com_icon_freship_ept"
        app:topicon = "@mipmap/menu_icon_02"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>



</RelativeLayout>
